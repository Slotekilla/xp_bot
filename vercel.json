{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/webhook/(.*)", "dest": "/api/webhook.js" },
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/bot/(.*)", "dest": "/api/bot.js" },
    { "src": "/api/echo", "dest": "/api/echo.js" }
  ]
}